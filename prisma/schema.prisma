generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Nitrogen {
  id        Int      @id @default(autoincrement())
  value     Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Phospor {
  id        Int      @id @default(autoincrement())
  value     Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Kalium {
  id        Int      @id @default(autoincrement())
  value     Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Ph {
  id        Int      @id @default(autoincrement())
  value     Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Temperature {
  id        Int      @id @default(autoincrement())
  value     Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Moisture {
  id        Int      @id @default(autoincrement())
  value     Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Conductivity {
  id        Int      @id @default(autoincrement())
  value     Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}


